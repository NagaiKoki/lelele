    "@commitlint/config-conventional": "^18.5.0",
    "commitizen": "^4.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "8.50.0",
    "happy-dom": "^13.3.1",
    "husky": "^8.0.0",
    "prettier": "3.0.3",
    "tsup": "^8.0.1",
    "turbo": "1.10.14",
